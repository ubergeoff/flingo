{
    "projectType": "library",
    "sourceRoot": "libs/workspace/source/src",
    "prefix": "rooi",
    "targets": {
        "test": {
            "executor": "@nrwl/jest:jest",
            "options": {
                "jestConfig": "libs/workspace/source/jest.config.ts",
                "passWithNoTests": true
            },
            "outputs": ["coverage/libs/workspace/source"]
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/workspace/source/**/*.ts", "libs/workspace/source/**/*.html"]
            }
        }
    },
    "generators": {
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "tags": []
}
