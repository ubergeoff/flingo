    "scripts": {
        "ng": "nx",
        "nx": "nx",
        "start": "nx run flingo:serve:dev --open",
        "build": "nx run flingo:build:production",
        "test": "ng test",
        "lint": "nx workspace-lint && ng lint",
        "e2e": "ng e2e",
        "affected:apps": "nx affected:apps",
