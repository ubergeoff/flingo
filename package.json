        "bump:slicker:patch": "cd libs/slicker && npm version patch --force",
        "build:muuri": "ng build muuri --configuration=production",
        "build:slick": "ng build slick --configuration=production",
        "build:slicker": "ng build slicker --configuration=production",
        "build:flingo": "ng build flingo --configuration=production",
        "publish:muuri": "npm publish --access public ./dist/libs/muuri",
        "publish:slick": "npm publish --access public ./dist/libs/slick",
        "publish:slicker": "npm publish --access public ./dist/libs/slicker",
        "workspace-generator": "nx workspace-generator"
